<% provide(:title, 'Profile') %>
<h1>Profile</h1>
<div class="row">
  <div class="span6 offset3">	
	<%= form_for(@profile) do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>
      <%= f.label :address %>
      <%= f.text_field :address %>
	  <%= f.label :city %>
	  <%= f.text_field :city%>
	  <%= f.label :state%>
	  <%= f.text_field :state%>
	  <%= f.label :zip%>
	  <%= f.text_field :zip%>
	 <%= f.submit%>
    <% end %>
  </div>
</div>
